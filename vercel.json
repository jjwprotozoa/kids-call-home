{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
            "dest": "/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html",
            "headers": {
                "Content-Type": "text/html"
            }
        }
    ],
    "env": {
        "PUSHER_APP_ID": "2048362",
        "PUSHER_KEY": "77f6120bb57b37175ae1",
        "PUSHER_SECRET": "06d9f40dc37f86821029",
        "PUSHER_CLUSTER": "eu",
        "VITE_PUSHER_KEY": "77f6120bb57b37175ae1",
        "VITE_PUSHER_CLUSTER": "eu"
    }
}